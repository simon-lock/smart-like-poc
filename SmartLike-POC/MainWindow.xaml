<Window x:Class="SmartLike_POC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:SmartLike_POC"
        xmlns:smartLike="clr-namespace:SmartLike_POC.SmartLike"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <local:MainWindowViewModel/>
    </Window.DataContext>
    
    <smartLike:SmartLikeView DataContext="{Binding SmartLike}"/>
    
</Window>
